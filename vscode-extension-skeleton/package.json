{
  "name": "vscode-extension-skeleton",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "swift",
        "aliases": [
          "Swift",
          "swift"
        ],
        "extensions": [
          ".swift"
        ]
      }
    ],
    "grammars": [
      {
        "language": "swift",
        "scopeName": "source.swift",
        "path": "../Syntaxes/Swift.tmLanguage"
      }
    ]
  }
}
